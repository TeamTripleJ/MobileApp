<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="com.circlesuite.httpwww.tempspinner.MainActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="61dp"
        android:background="@android:color/holo_blue_light"
        android:minHeight="?attr/actionBarSize"
        android:layout_gravity="left"
        android:theme="?attr/actionBarTheme"
        android:textColor="@android:color/white"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    <Spinner
        android:id="@+id/spinner"
        android:layout_width="51dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="@android:color/white"
        />

    </android.support.v7.widget.Toolbar>



    <TextView
        android:id="@+id/Name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Firstname"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="Older"
        android:textSize="8sp"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="126dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="135dp"
        app:layout_constraintLeft_toLeftOf="parent" />



    <HorizontalScrollView
        android:id="@+id/horizontalscrollView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="100dp"

        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="@+id/toolbar">

        <LinearLayout
            android:id="@+id/LinearLayout04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="53dp"
                android:layout_height="41dp"
                android:background="#ffffff"
                app:srcCompat="@drawable/home"
                tools:layout_editor_absoluteX="91dp"
                tools:layout_editor_absoluteY="16dp" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="61dp"
                android:layout_height="42dp"
                android:backgroundTint="#ffffff"
                app:srcCompat="@drawable/post"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="16dp" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="55dp"
                android:layout_height="47dp"
                android:background="#ffffff"
                app:srcCompat="@drawable/schedule"
                tools:layout_editor_absoluteX="233dp"
                tools:layout_editor_absoluteY="16dp" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="49dp"
                android:layout_height="43dp"
                android:background="#ffffff"
                app:srcCompat="@drawable/refresh"
                tools:layout_editor_absoluteX="304dp"
                tools:layout_editor_absoluteY="16dp" />
        </LinearLayout>
    </HorizontalScrollView>

    <HorizontalScrollView
        android:id="@+id/horizontalscrollView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="70dp"
        android:paddingLeft="120dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="@+id/toolbar">

        <LinearLayout
            android:id="@+id/LinearLayout05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="49dp"
                android:layout_height="44dp"
                app:srcCompat="@drawable/fb1"
                tools:layout_editor_absoluteX="63dp"
                tools:layout_editor_absoluteY="70dp" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                app:srcCompat="@drawable/twitter"
                tools:layout_editor_absoluteX="126dp"
                tools:layout_editor_absoluteY="70dp" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                app:srcCompat="@drawable/gram"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="70dp" />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                app:srcCompat="@drawable/in"
                tools:layout_editor_absoluteX="241dp"
                tools:layout_editor_absoluteY="70dp" />
        </LinearLayout>
    </HorizontalScrollView>


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recent Activity"
        android:paddingLeft="150dp"
        android:paddingTop="125dp"
        />

    <ScrollView
        android:id="@+id/ScrollView02"
        android:layout_width="361dp"
        android:layout_height="160dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
       >

        <LinearLayout
            android:id="@+id/feedContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>
    </ScrollView>



</android.support.constraint.ConstraintLayout>
